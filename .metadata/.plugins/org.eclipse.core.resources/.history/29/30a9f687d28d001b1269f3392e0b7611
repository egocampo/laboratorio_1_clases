/* Para que una funcion sea privada de un archivo.c la incluis en dicho archivo como: static int funcion();
 *
 * */
#include <stdio.h>
#include <stdlib.h>
#include <stdio_ext.h>
#include "utn.h"  // para mis bibliotecas se usan comillas

int main(void) {
	setbuf(stdout,NULL);// siempre
	float numero;
	if(utn_getFloat(&numero,"Ingrese edad: ","Error papu, n√∫mero fuera del rango\n",UTN_REINTENTOS,UTN_MIN_EDAD,UTN_MAX_EDAD)==0)
	{
		printf("Ingresaste %d\n",numero);
	}
	return EXIT_SUCCESS;
}
